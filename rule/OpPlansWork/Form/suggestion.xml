<?xml version="1.0" encoding="utf-8"?>
<rule type="form" id="suggestion" mode="on" history="on">
    <description>Suggestion</description>
    <xsltfile>forms/suggestion.xsl</xsltfile>
    <doctype>main</doctype>

    <events>
        <queryopen source="file">supa_nd.plans_work.form.suggestion.QueryOpen</queryopen>
        <querysave source="file">supa_nd.plans_work.form.suggestion.QuerySave</querysave>
    </events>

    <caption name="title" source="KEYWORD">Предложение</caption>
    <caption name="fio" source="KEYWORD">ФИО</caption>
    <caption name="sex" source="KEYWORD">Пол</caption>
    <caption name="age" source="KEYWORD">Возраст</caption>
    <caption name="saveclose" source="KEYWORD">Сохранить и закрыть</caption>
    <caption name="close" source="KEYWORD">Закрыть</caption>
</rule>
