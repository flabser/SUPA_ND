<?xml version="1.0" encoding="UTF-8"?>
<rule id="OpPlansWork" mode="on" type="global">
    <description>Операционные планы и планы работы подразделений</description>
    <orgname>ТОО "Lab of the Future"</orgname>
    <liccount>100</liccount>
    <logo>workflow_small.png</logo>
    <database autodeploy="ON">
        <name>USTP</name>
        <driver>org.postgresql.Driver</driver>
        <url>jdbc:postgresql://localhost:5433/USTP</url>
        <username>postgres</username>
        <password>1</password>
    </database>
    <externalmodule type="STRUCTURE">Structure</externalmodule>
    <entrypoint>Provider?type=static&amp;id=start</entrypoint>
    <defaultredirecturl>Provider?type=page&amp;id=suggestions&amp;page=0</defaultredirecturl>
    <langs>
        <entry id="RUS" isprimary="1">Русский</entry>
        <entry id="KAZ">Казакша</entry>
        <entry id="ENG">English</entry>
    </langs>
    <skins>
        <entry default="on" errorpage="error.xsl" id="classic" path="./webapps/OpPlansWork/classic/xsl">Classic</entry>
    </skins>
    <roles>
        <entry id="administrator">Разрешает ввод и редактирование значений справочников</entry>
    </roles>
    <markdelaysec>5</markdelaysec>
    <eds mode="OFF">
        <keypass/>
        <keystore/>
        <ocsp>http://ocsp.pki.kz:80</ocsp>
    </eds>
    <ocsp>http://ocsp.pki.kz:80</ocsp>
</rule>
