<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="suggestions" mode="on" history="on">
    <description>suggestions</description>
    <caching>NO_CACHING</caching>
    <xsltfile>operational_plans_work/pages/suggestions.xsl</xsltfile>

    <element mode="ON" type="INCLUDED_PAGE">
        <name>outline</name>
        <value>outline</value>
    </element>
    <element mode="ON" type="SCRIPT">
        <name>view_content</name>
        <events>
            <doscript source="file">supa_nd.plans_work.page.Suggestions</doscript>
        </events>
    </element>

    <caption name="title" source="KEYWORD">Журнал для сбора предложений</caption>
    <caption name="viewtext1" source="KEYWORD">Мероприятия, пути и средства достижения цели</caption>
    <caption name="viewtext2" source="KEYWORD">Ответственный исполнитель</caption>
    <caption name="viewtext4" source="KEYWORD">Срок исполнения</caption>
</rule>
