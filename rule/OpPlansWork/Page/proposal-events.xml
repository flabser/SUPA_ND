<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="proposal-events" mode="on" history="off">
    <description>proposal-events</description>
    <caching>NO_CACHING</caching>
    <xsltfile>pages/proposal-events.xsl</xsltfile>

    <element mode="ON" type="SCRIPT">
        <name>view_content</name>
        <events>
            <doscript source="file">supa_nd.plans_work.page.ProposalEvents</doscript>
        </events>
    </element>

    <caption name="coord_start" source="KEYWORD">Отправлено на согласование</caption>
    <caption name="coord_agree" source="KEYWORD">Согласен</caption>
    <caption name="coord_revision" source="KEYWORD">Возврат на доработку</caption>
    <caption name="coord_reject" source="KEYWORD">Исключение</caption>

    <caption name="changed" source="KEYWORD">Изменено</caption>

    <caption name="description" source="KEYWORD">Описание</caption>
    <caption name="assignee" source="KEYWORD">Исполнитель</caption>
    <caption name="due_date" source="KEYWORD">Срок исполнения</caption>
</rule>
