<?xml version="1.0" encoding="utf-8"?>
<rule type="page" id="getdiffctrldate" mode="on" history="off">
	<description>Количество оставшихся дней до конца срока исполнения</description>
	<rununderuser source="static">[supervisor]</rununderuser>
	<caching>NO_CACHING</caching>
	<xsltfile></xsltfile>
	<element mode="ON" type="SCRIPT">
		<name>script_result</name>
		<events>
			<doscript source="file">page.getdiffctrldate.DoScript</doscript>
		</events>
	</element>
</rule>
